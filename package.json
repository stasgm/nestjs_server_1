{
  "name": "nest-sequelize-typescript",
  "version": "0.0.1",
  "description": "Nest + sequelize-typescript + JWT + Jest + Swagger",
  "keywords": [
    "nest",
    "nestjs",
    "nest.js",
    "sequelize",
    "sequelize-typescript",
    "orm",
    "nodejs",
    "node.js",
    "node",
    "typescript",
    "jwt",
    "jsonwebtoken",
    "passport",
    "swagger",
    "jest"
  ],
  "author": "Kent Loog",
  "license": "MIT",
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "format": "prettier --write \"src/**/*.ts\"",
    "db:migrate": "ts-node node_modules/.bin/sequelize db:migrate",
    "db:drop": "yarn build && node_modules/.bin/sequelize db:drop",
    "db:drop2": "ts-node node_modules/.bin/sequelize db:drop",
    "db:create": "ts-node node_modules/.bin/sequelize db:create",
    "db:seed-dev": "ts-node node_modules/.bin/sequelize db:seed:all",
    "db:seed-prod": "ts-node node_modules/.bin/sequelize db:seed:all --seeders-path db/seeders-prod",
    "db:reset": "yarn db:drop && npm run db:create && npm run db:migrate && npm run db:seed-dev && npm run db:seed-prod",
    "start": "ts-node -r tsconfig-paths/register src/main.ts",
    "start:dev": "nodemon",
    "start:debug": "nodemon --config nodemon-debug.json",
    "prestart:prod": "rimraf dist && npm run build",
    "start:prod": "node dist/src/main.js",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "test": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@nestjs/common": "^8.0.6",
    "@nestjs/core": "^8.0.6",
    "@nestjs/passport": "^8.0.1",
    "@nestjs/platform-express": "^8.0.6",
    "@nestjs/swagger": "^5.0.9",
    "bcrypt": "^5.0.1",
    "class-transformer": "^0.4.0",
    "class-validator": "^0.13.1",
    "dotenv": "^10.0.0",
    "jsonwebtoken": "^8.5.1",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "pg": "^8.7.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.3.0",
    "sequelize": "^6.6.5",
    "sequelize-cli": "^6.2.0",
    "sequelize-typescript": "^2.1.0",
    "swagger-ui-express": "^4.1.6"
  },
  "devDependencies": {
    "@nestjs/testing": "^8.0.6",
    "@types/bcrypt": "^5.0.0",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.0.1",
    "@types/jsonwebtoken": "^8.5.4",
    "@types/node": "^16.6.1",
    "@types/passport": "^1.0.7",
    "@types/passport-jwt": "^3.0.6",
    "@types/sequelize": "^4.28.10",
    "@types/supertest": "^2.0.11",
    "jest": "^27.0.6",
    "nodemon": "^2.0.12",
    "prettier": "^2.3.2",
    "supertest": "^6.1.5",
    "ts-jest": "^27.0.4",
    "ts-node": "^10.2.0",
    "tsconfig-paths": "^3.10.1",
    "tslint": "6.1.3",
    "typescript": "^4.3.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
